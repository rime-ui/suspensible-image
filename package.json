        "push": "git push --follow-tags origin main && npm publish"
    },
    "dependencies": {
        "react": "^19.0.0",
        "react-dom": "^19.0.0"
    },
    "devDependencies": {
        "@commitlint/config-conventional": "^19.8.0",
