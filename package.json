        "@types/react": "^19.0.10",
        "@types/react-dom": "^19.0.4",
        "commitizen": "^4.3.1",
        "commitlint": "^19.8.0",
        "eslint": "^9.22.0",
        "eslint-plugin-react-hooks": "^5.2.0",
        "eslint-plugin-react-refresh": "^0.4.19",
        "globals": "^16.0.0",
        "standard-version": "^9.5.0",
        "tsup": "^8.4.0",
        "typescript": "~5.7.2",
        "typescript-eslint": "^8.26.1"
    }
