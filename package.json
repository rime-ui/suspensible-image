{
    "name": "@rime-ui/suspensible-image",
    "private": false,
    "version": "0.0.10",
    "description": "A React image component with smooth skeleton loading",
    "type": "module",
    "author": "Ahmed Hany",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "files": [
        "dist"
    ],
    "scripts": {
